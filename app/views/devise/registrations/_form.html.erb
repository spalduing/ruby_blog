     <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render "devise/shared/error_messages", resource: resource %>
        <div class="form-group mb-3">
          <%= f.label :email, class: "form-label", for:"emailValidation" %><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control form-control-lg ", id:"emailValidation", required: true %>
        </div>
        <div class="form-group mb-3">
          <%= f.label :password, class: "form-label", for:"passwordValidation" %>
          <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "new-password", class:"form-control form-control-lg ", id:"passwordValidation", required: true %>
        </div>
        <div class="form-group mb-3">
          <%= f.label :password_confirmation, class: "form-label", for:"confirmPaswordValidation" %><br />
          <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control form-control-lg ", id:"confirmPaswordValidation", required: true %>
        </div>
        <div class="form-group mb-3">
          <%= f.submit "Sign up", class:"btn btn-primary btn-lg" %>
        </div>
      <% end %>